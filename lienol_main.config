CONFIG_PACKAGE_luci-app-serverchan=y
CONFIG_PACKAGE_luci-app-poweroff=y
CONFIG_PACKAGE_luci-app-verysync=y

# 主题
# CONFIG_PACKAGE_luci-theme-argon is not set

